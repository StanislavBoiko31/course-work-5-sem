{
  "name": "photo-studio-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-datepicker": "^8.9.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.js\" --fix",
    "lint:quiet": "eslint \"src/**/*.js\" --quiet",
    "lint:report": "eslint \"src/**/*.js\" > eslint_report.txt",
    "lint:json": "eslint \"src/**/*.js\" --format json > eslint_report.json",
    "lint:html": "eslint \"src/**/*.js\" --format html > eslint_report.html",
    "lint:compact": "eslint \"src/**/*.js\" --format compact",
    "lint:pages": "eslint \"src/pages/**/*.js\" --format json > eslint_pages_report.json",
    "lint:components": "eslint \"src/components/**/*.js\" --format json > eslint_components_report.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8000"
}
